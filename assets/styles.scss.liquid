  @mixin border-right($width, $style, $color) {
    border-right: $width $style $color;
  }

  @mixin border-bottom($width, $style, $color) {
    border-bottom: $width $style $color;
  }

  @mixin border-top($width, $style, $color) {
    border-top: $width $style $color;
  }

